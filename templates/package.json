		"prisma-field-encryption": "^1.6.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"redaxios": "^0.5.1",
		"vitest": "^3.2.4",
		"zod": "^3.25.71"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.6",
