{
	"name": "my-mittwald-extension",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "pnpm db:generate && vite build && tsc --noEmit",
		"start": "vite start",
		"check": "pnpm biome check .",
		"db:generate": "prisma generate",
		"db:format": "prisma format",
