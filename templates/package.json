		"@types/react": "^19.0.8",
		"@types/react-dom": "^19.0.3",
		"prisma": "6.10.1",
		"typescript": "^5.7.2",
		"vite": "^6.4.1",
		"vite-tsconfig-paths": "^5.1.4"
